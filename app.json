{
  "expo": {
    "name": "WaveDistrict Native",
    "slug": "wd-native",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "description": "WaveDistrict native app backed by a GraphQL mirror https://github.com/kingdaro/wavedistrict-graphql-mirror",
    "githubUrl": "https://github.com/kingdaro/wd-native"
  }
}
